{
  "_version": "1.12.0",
  "sap.app": {
    "id": "sap.ui.demo.practice",
    "type": "application",
    "i18n": "i18n/i18n.properties",
    "title": "{{appTitle}}",
    "description": "{{appDescription}}",
    "applicationVersion": {
      "version": "1.0.0"
    }
  },
  "sap.ui": {
    "technology": "UI5",
    "deviceTypes": {
      "desktop": true,
      "tablet": true,
      "phone": true
    }
  },
  "sap.ui5": {
    "css": [
      {
        "uri": "css/style.scss"
      }
    ],
    "rootView": {
      "viewName": "sap.ui.demo.practice.view.App",
      "type": "XML",
      "id": "app"
    },
    "dependencies": {
      "minUI5Version": "1.93",
      "libs": {
        "sap.ui.core": {},
        "sap.m": {},
        "sap.f": {},
        "sap.suite.ui.microchart": {}
      }
    },
    "models": {
      "i18n": {
        "type": "sap.ui.model.resource.ResourceModel",
        "settings": {
          "bundleName": "sap.ui.demo.practice.i18n.i18n",
          "supportedLocales": [""],
          "fallbackLocale": ""
        }
      }
    },
    "routing": {
      "config": {
        "routerClass": "sap.m.routing.Router",
        "viewType": "XML",
        "viewPath": "sap.ui.demo.practice.view",
        "controlId": "app",
        "controlAggregation": "pages",
        "transition": "slide",
        "bypassed": {
          "target": "notFound"
        },
        "async": true
      },
      "routes": [{
        "pattern": "",
        "name": "appHome",
        "target": "home"
      },{
        "pattern": "corona",
        "name": "corona",
        "target": "corona"
      },{
        "pattern": "other",
        "name": "otherViews",
        "target": "other"
      }],
      "targets": {
        "home": {
          "viewId": "home",
          "viewName": "Home",
          "viewLevel" : 1
        },
        "notFound": {
          "viewId": "notFound",
          "viewName": "NotFound",
          "transition": "show"
        },
        "corona": {
          "viewId": "corona",
          "viewName": "Corona",
          "viewLevel" : 1
        },
        "other": {
          "viewId": "other",
          "viewName": "Overview",
          "viewLevel" : 1
        }
      }
    }
  }
}
