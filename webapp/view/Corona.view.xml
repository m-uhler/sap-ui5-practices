<mvc:View
        controllerName="sap.ui.demo.practice.controller.Corona"
        xmlns="sap.m"
        xmlns:table="sap.ui.table"
        xmlns:mvc="sap.ui.core.mvc"
        displayBlock="true"
>
    <Page title="{i18n>coronaStats}" showNavButton="true" navButtonPress="onNavBack">
        <Panel class="sapUiResponsiveMargin" width="auto" showNavButton="true"
               navButtonPress="onNavBack">
            <table:Table selectionMode="None" rows="{corona>/}">
                <table:columns>
                    <table:Column>
                        <Label text="{i18n>date}" />
                        <table:template>
                            <Label text="{corona>date}" />
                        </table:template>
                    </table:Column>
                    <table:Column>
                        <Label text="{i18n>positive}" />
                        <table:template>
                            <Label text="{corona>positive}" />
                        </table:template>
                    </table:Column>
                    <table:Column>
                        <Label text="{i18n>negative}" />
                        <table:template>
                            <Label text="{corona>negative}" />
                        </table:template>
                    </table:Column>
                </table:columns>
            </table:Table>
        </Panel>
    </Page>
</mvc:View>